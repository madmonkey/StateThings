@model StateInterface.Areas.Design.Models.TransactionSnippetModel
<div class="padLeft padRight">
    <div class="row">
        <div class="col-xs-12">
            <div class="pull-right">
                <a target="_blank" class="glyphicon glyphicon-question-sign" title="Help" href="@Url.Action("Help")"></a>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <div data-bind="template: { name: 'snippetProperties-template' }"></div>
            <div data-bind="template: { name: 'snippetField-template' }"></div>
        </div>
    </div>
</div>
<script type="text/javascript">
    var initialData = @Html.Raw(Model.InitialData)
</script>
@Html.Partial("_snippetPropertiesTemplate")
@Html.Partial("_snippetFieldTemplate")
@Html.Partial("_snippetFieldModal")
@Html.Partial("_snippetPropertiesModal")
@Html.Partial("_confirmDeleteModal")

@section scripts{
    @Scripts.Render("~/bundles/stateinterface_design_snippetfield")
}